@{
    ViewData["Title"] = "Reporte de Tracking de Gestión";
}

<script src="https://code.jquery.com/jquery-3.7.0.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container mt-4">
    <h2>Reporte de Tracking</h2>

    <div class="row mt-4">
        <div class="col-md-4">

            <label>Número de Gestión:</label>
            <input id="gestionId" class="form-control" type="number" placeholder="Ingrese ID de gestión" />

            <button class="btn btn-primary mt-3" onclick="buscarTracking()">
                Buscar Tracking
            </button>
        </div>
    </div>
</div>

<script src="~/js/reporte.js"></script>
